    "@types/string_score": "^0.1.31",
    "@types/uuid": "^9.0.8",
    "@types/vscode": "^1.70.0",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-cli": "^0.0.6",
